<keymap version="1" name="GNOME copy" parent="Default for GNOME">
  <action id="Console.TableResult.ColumnVisibility" />
  <action id="Console.TableResult.SelectRow" />
  <action id="GotoFile">
    <keyboard-shortcut first-keystroke="shift ctrl n" />
    <keyboard-shortcut first-keystroke="space" second-keystroke="f" />
  </action>
  <action id="GotoNextError">
    <keyboard-shortcut first-keystroke="f2" />
    <keyboard-shortcut first-keystroke="space" second-keystroke="e" />
  </action>
  <action id="GotoPreviousError">
    <keyboard-shortcut first-keystroke="shift f2" />
    <keyboard-shortcut first-keystroke="shift space" second-keystroke="e" />
  </action>
  <action id="Switcher">
    <keyboard-shortcut first-keystroke="ctrl tab" />
    <keyboard-shortcut first-keystroke="shift ctrl tab" />
    <keyboard-shortcut first-keystroke="space" second-keystroke="space" />
  </action>
  <action id="ToggleZenMode">
    <keyboard-shortcut first-keystroke="space" second-keystroke="z" />
  </action>
</keymap>